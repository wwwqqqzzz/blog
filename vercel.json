{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "pnpm install --no-frozen-lockfile",
        "buildCommand": "NODE_ENV=production CI=false DISABLE_ESLINT_PLUGIN=true pnpm build",
        "outputDirectory": "build"
      }
    }
  ]
}
